<classMaker classVarName="component">
    <anno classVarName="componentAnno">
        <param key="modules" classVarName="module"/>
        <param key="dependencies" classVarName="appComponent"/>
    </anno>
    <anno classVarName="activityScope"/>

    <note>${note}</note>

    <method returnClassName="void" methodName="inject${type}">
        <methodParam classVarName="fragment" valueName="fragment"/>
    </method>

    <method returnClassName="void" methodName="injectPresenter">
        <methodParam classVarName="presenter" valueName="presenter"/>
    </method>
</classMaker>